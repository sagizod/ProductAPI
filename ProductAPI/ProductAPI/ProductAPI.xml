<?xml version="1.0"?>
<doc>
    <assembly>
        <name>ProductAPI</name>
    </assembly>
    <members>
        <member name="M:ProductAPI.Controllers.ProductController.GetProduct(System.Int32)">
            <summary>
            Finds a product with a matching id
            </summary>
            <param name="id">Product id to find details for</param>
            <returns>Product with matching id, if present</returns>
            <response code="200">Product with ID found</response>
            <response code="404">Product with ID not found</response>
        </member>
        <member name="M:ProductAPI.Controllers.ProductController.PutsProduct(System.Int32,ProductAPI.Models.Product)">
            <summary>
            Updates a product with a matching id
            </summary>
            <param name="id">Id of the product to be updated</param>
            <param name="newproduct"></param>
            <returns></returns>
            <response code="200">Product updated successfully</response>
            <response code="404">Product with ID not found</response>
            <response code="409">Conflict on updating product</response>
        </member>
        <member name="M:ProductAPI.Controllers.ProductController.PostProduct(ProductAPI.Models.Product)">
            <summary>
            Create a new product entry
            </summary>
            <param name="product">Product details, do not send Id data</param>
            <returns></returns>
            <response code="200">Product created successfully</response>
            <response code="400">Product data incompatible</response>
            <response code="409">Error creating product</response>
        </member>
        <member name="M:ProductAPI.Controllers.ProductController.DeleteProduct(System.Int32)">
            <summary>
            Deletes a product with a matching id
            </summary>
            <param name="id">Id of the product to be deleted</param>
            <returns></returns>
            <response code="200">Product with ID deleted</response>
            <response code="404">Product with ID not found</response>
        </member>
        <member name="M:ProductAPI.Controllers.ProductsController.GetProducts">
            <summary>
            Get list of all products
            </summary>
            <returns>List of Products</returns>
            <response code="200">Success</response>
        </member>
        <member name="T:ProductAPI.Models.Product">
            <summary>
            Model for product
            </summary>
        </member>
        <member name="P:ProductAPI.Models.Product.Id">
            <summary>
            Product ID - generated by database, leave null
            </summary>
        </member>
        <member name="P:ProductAPI.Models.Product.Name">
            <summary>
            Product Name
            </summary>
        </member>
        <member name="P:ProductAPI.Models.Product.Price">
            <summary>
            Product Price, in pounds
            </summary>
        </member>
    </members>
</doc>
